{
  "name": "fetch.macro",
  "description": "write fetch function with babel.macros",
  "author": "r17x <hi@rin.rocks>",
  "version": "0.0.0",
  "keywords": [
    "babel-plugin-macros",
    "fetch"
  ],
  "main": "src/fetch.macro.js",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint",
    "test": "jest"
  },
  "prettier": {
    "endOfLine": "auto",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-kdnj"
    ],
    "root": true
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.js": [
      "eslint --fix",
      "cross-env NODE_ENV=test jest --bail --findRelatedTests"
    ]
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "tests/create-tests.js"
    ]
  },
  "peerDependencies": {
    "babel-plugin-macros": "^2"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "babel-plugin-macros": "^3",
    "babel-plugin-tester": "^10",
    "cross-env": "^7",
    "eslint": "^8",
    "eslint-config-kdnj": "^2",
    "prettier": "^2",
    "husky": "^8",
    "jest": "^29",
    "lint-staged": "^13"
  }
}
